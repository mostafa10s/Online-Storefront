{
  "name": "DISHA-STORE",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon ./src/index.ts",
    "clean": "rimraf build/",
    "watch-tsc": "yarn clean && npx tsc --watch",
    "nodemon-js": "nodemon build/index.js",
    "build": "yarn clean && npx tsc",
    "start": "npm run build && nodemon build/index.js",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "migration:run": "db-migrate up",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "create-test-env": "yarn build && db-migrate db:drop DISHASTORE_TEST && db-migrate db:create DISHASTORE_TEST && db-migrate -e test up",
    "run-jasmine": "cross-env NODE_ENV=test yarn jasmine",
    "test": "yarn create-test-env && yarn run-jasmine"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
